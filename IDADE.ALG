Algoritmo "Idade"
// Autor(a)    : Gabriel S A Matos
// Data atual  : 28/09/2022
Var
   ano, nasc, idade: inteiro


Inicio
      Escreva ("Digite ao lado o ano em que estamos:")
      leia (ano)
      Escreva ("Agora digite ao lado o ano em que você nasceu:")
      leia (nasc)

      idade <- ano - nasc
      Escreva ("Você fará", idade, " anos esse ano!")


Fimalgoritmo