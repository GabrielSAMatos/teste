Algoritmo "soma"
// Autor(a)    : Gabriel S A Matos
// Data atual  : 29/09/2022
Var
   soma, cont, num: inteiro


Inicio
   soma <- 0
   cont <- 1
   Enquanto (cont <=10) faca
      Escreva ("Digite o ", cont, "º número:")
      leia (num)
      soma <- soma + num
      cont <- cont + 1
   fimenquanto
   Escreva ("A soma de todos os valores foi:", soma)



Fimalgoritmo